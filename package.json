{
  "name": "resume-bot",
  "version": "1.0.0",
  "description": "An AI-powered resume generator that helps create professional resumes",
  "main": "server/index.js",
  "scripts": {
    "start": "node server/index.js",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "nodemon server/index.js",
    "client": "cd client && npm start",
    "build": "cd client && npm run build"
  },
  "keywords": [
    "resume",
    "ai",
    "generator",
    "cv",
    "career"
  ],
  "author": "westbrookcook",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "openai": "^4.20.1"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "concurrently": "^8.2.2"
  }
}